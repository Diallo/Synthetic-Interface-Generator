import "V1.signature"

interface V1 version "1.0"


		
machine V1 {

	initial state p1_1{
			transition
				trigger: inTRp1_1_0
				next state: p1_2
			
			}

	 state p1_2{
			transition
				do: outTRp1_2_2
				next state: p1_3
			}

	 state p1_4{
			transition
				do: outTRp1_2_1
				next state: p1_1
			}

	 state p1_3{
			transition
				trigger: inTRp1_3_3
				next state: p1_4
			
			}
}
